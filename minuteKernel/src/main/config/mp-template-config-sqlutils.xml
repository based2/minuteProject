<target name="SQL">		
	<architecture-target name="sql"></architecture-target>	
	<plugin name="sqlUtils" className="net.sf.minuteProject.utils.SqlUtils"></plugin>
	
  	<template-target 
    	name="SQL scripts"
		outputdir="scripts">
			 
		<template name="TableOutOfViewSQL" templateFileName="TableOutOfViewSQL.vm" 
			add-model-name="true" package-root=""
			add-model-dir-name="false"		
			subdir="" outputsubdir="" technicalPackage=""
			file-extension="sql" file-prefix="" file-suffix="-table-out-of-view" 
			model-specific="true">
		</template>
			 
		<template name="SQL.ResetAndCreateForeignKeys" templateFileName="SQL.ResetAndCreateForeignKeys.vm" 
			add-model-name="true" package-root=""
			add-model-dir-name="false"		
			subdir="" outputsubdir="" technicalPackage=""
			file-extension="sql" file-prefix="" file-suffix="-table" 
			model-specific="true">
		</template>
			 
		<template name="SQL.Trigger" templateFileName="SQL.Trigger.vm" 
			add-model-name="true" package-root=""
			add-model-dir-name="false"		
			subdir="" outputsubdir="" technicalPackage=""
			file-extension="sql" file-prefix="" file-suffix="-triggers" 
			model-specific="true">
		</template>
								
	</template-target>	

</target>