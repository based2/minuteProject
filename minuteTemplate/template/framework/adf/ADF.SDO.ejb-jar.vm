#exposeSDOSpecific()
#set($connectionDS=$template.getPropertyValue("connection-datasource-name","jdbc/${model.name}DS"))
<?xml version = '1.0' encoding = 'windows-1252'?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         version="3.0" xmlns="http://java.sun.com/xml/ns/javaee">
  <enterprise-beans>
    <session>
##      <ejb-name>${appModuleADFSODServiceFaceImport}</ejb-name>
      <ejb-name>${ejbBean}</ejb-name>
      <resource-ref>
        <res-ref-name>$connectionDS</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
      </resource-ref>
    </session>
  </enterprise-beans>
</ejb-jar>
